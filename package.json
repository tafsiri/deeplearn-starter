{
  "name": "deeplearn.js-starter-project",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "deeplearn": "^0.5.0",
    "parcel-bundler": "^1.6.2"
  },
  "scripts": {
    "start": "NODE_ENV=development parcel --no-hmr index.html ",
    "build": "NODE_ENV=production parcel build index.html  --no-minify --public-url /"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "typescript": "^2.7.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 3 versions",
              "ie >= 9",
              "ios >= 8",
              "android >= 4.2"
            ]
          },
          "useBuiltIns": false
        }
      ]      
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
